{
  "name": "dapp-scaffold",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "@craco/craco": "^6.1.2",
    "@ledgerhq/hw-transport-webusb": "^5.53.1",
    "@project-serum/anchor": "0.17.1-beta.1",
    "@project-serum/common": "^0.0.1-beta.3",
    "@project-serum/serum": "^0.13.60",
    "@project-serum/sol-wallet-adapter": "^0.2.6",
    "@solana/spl-token": "0.1.8",
    "@solana/spl-token-registry": "^0.2.280",
    "@solana/spl-token-swap": "0.1.2",
    "@solana/web3.js": "^1.29.2",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@types/react-router-dom": "^5.1.6",
    "antd": "^4.6.6",
    "bn.js": "^5.2.0",
    "borsh": "^0.4.0",
    "bs58": "^4.0.1",
    "buffer-layout": "^1.2.0",
    "craco-less": "^1.17.0",
    "eventemitter3": "^4.0.7",
    "jazzicon": "^1.5.0",
    "mz": "^2.7.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "yaml": "^1.10.2"
  },
  "scripts": {
    "start": "cd contract && anchor launch --program-name circlepod_protocol",
    "start:drops-reward": "ts-node contract/client/drops-reward.ts",
    "start:lockup-init": "ts-node contract/client/lockup-init.ts",
    "start:create-new-pool": "ts-node contract/client/create-new-pool.ts",
    "start:ido:process": "ts-node contract/client/ido-process.ts",
    "start:ido:process:test": "NODE_ENV=test ts-node contract/client/ido-process.ts",
    "start:ido:init:test": "NODE_ENV=test ts-node contract/client/ido-init.ts",
    "registry-init": "ts-node contract/client/registry-init.ts",
    "clean": "cd contract && rm -rf target/rls target/debug target/idl target/release target/deploy/*.so",
    "lanch": "yarn build:anchor && yarn deploy:solana && yarn deploy:anchor",
    "build:anchor": "cd contract && anchor build",
    "build:anchor:clean": "yarn clean && cd contract && anchor build",
    "deploy:anchor": "yarn deploy:anchor:cp && yarn deploy:anchor:staking_lockup && yarn deploy:anchor:staking",
    "deploy:anchor:cp": "cd contract && anchor deploy -p circlepod-protocol",
    "deploy:anchor:staking_lockup": "cd contract && anchor deploy -p staking-lockup",
    "deploy:anchor:staking": "cd contract && anchor deploy -p staking-registry",
    "deploy:solana": "yarn deploy:solana:cp && yarn deploy:solana:staking_lockup && yarn deploy:solana:staking && yarn deploy:solana:ido",
    "deploy:solana:cp": "solana program deploy -v contract/target/deploy/token_proxy.so",
    "deploy:solana:staking_lockup": "solana program -v deploy contract/target/deploy/staking_lockup.so",
    "deploy:solana:staking": "solana program deploy -v contract/target/deploy/staking_registry.so",
    "deploy:solana:ido": "solana program deploy -v contract/target/deploy/ido_sale.so",
    "test:cp": "cd contract && anchor test --skip-deploy --skip-build --skip-local-validator --skip-local-validator ./tests/circlepod-protocol.js",
    "test:lockup": "cd contract && anchor test --skip-deploy --skip-build --skip-local-validator --skip-local-validator ./tests/lockup.js",
    "test:ido": "cd contract && anchor test --skip-deploy --skip-build --skip-local-validator ./tests/ido-sale.js",
    "eject": "react-scripts eject",
    "localnet:logs": "solana logs",
    "switch:dev": "solana config set --url https://api.devnet.solana.com",
    "switch:local": "solana config set --url localhost",
    "predeploy": "git pull --ff-only && yarn && yarn build",
    "deploy": "yarn switch:dev && yarn deploy:solana && yarn switch:local",
    "format:fix": "prettier --write \"**/*.+(js|jsx|ts|tsx|json|css|md)\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/solana-labs/dapp-scaffold"
  },
  "homepage": ".",
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@types/bn.js": "^4.11.6",
    "@types/bs58": "^4.0.1",
    "@types/eslint": "^7.2.4",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/jest": "^24.9.1",
    "@types/ledgerhq__hw-transport": "^4.21.3",
    "@types/ledgerhq__hw-transport-webusb": "^4.70.1",
    "@types/mz": "^2.7.2",
    "@types/node": "^15.6.1",
    "@types/prettier": "^2.1.5",
    "@types/react": "^16.14.8",
    "@types/react-dom": "^16.9.13",
    "@types/yaml": "^1.9.7",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "arweave-deploy": "^1.9.1",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "gh-pages": "^3.1.0",
    "mocha": "^8.4.0",
    "prettier": "^2.1.2",
    "start-server-and-test": "^1.11.6",
    "ts-node": "^9.1.1",
    "typescript": "^4.0.5"
  }
}
